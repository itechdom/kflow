  },
  "homepage": "https://github.com/itechdom/playground#readme",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.580.0",
    "@markab.io/node": "^13.0.2",
    "@markab.io/orbital-api": "^7.0.1",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "config": "^3.3.11",
    "cors": "^2.8.5",
    "express": "^4.14.1",
    "express-print-routes": "^1.0.0",
